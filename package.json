{
  "name": "synapse-ai-coach-v2",
  "version": "2.0.0",
  "description": "Advanced AI learning platform with multi-agent coaching system",
  "main": "server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec \"ts-node --transpile-only src/server.ts\"",
    "build": "tsc",
    "test": "jest",
    "test-api": "node test-api.js",
    "test-basic": "node test-without-api-key.js",
    "db:init": "node scripts/init-db.js",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all",
    "setup": "npm install && npm run build && echo 'Setup complete! Run: npm run dev'"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.0",
    "cheerio": "^1.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mailparser": "^3.7.4",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "openai": "^5.10.1",
    "pg": "^8.11.3",
    "puppeteer": "^24.14.0",
    "rss-parser": "^3.13.0",
    "sequelize": "^6.35.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "ai",
    "learning",
    "coaching",
    "multi-agent",
    "education"
  ],
  "author": "Synapse Team",
  "license": "MIT"
}
